

<div id="<%= calendar %>" class = "calendar-overview">
  <p>
    <strong>Name:</strong>
    <%= calendar.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= calendar.description %>
  </p>

  <p>
    <strong>Creator:</strong>
     <%=  User.find_by(id: calendar.user_id).first_name + " " + User.find_by(id: calendar.user_id).last_name %>
  </p>

  <p>
    <strong>Public:</strong>
    <% if calendar.shared %>
      <td> Public</td>
    <% else%>
      <td> Not public </td>
    <%end%>
  </p>

</div>